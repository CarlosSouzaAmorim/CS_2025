#include <stdio.h>

int main() {
    FILE *arquivo;
    arquivo = fopen("file1c", "r"); // Abre o arquivo para leitura

    if (arquivo == NULL) {
        printf("Erro ao abrir o arquivo.\n");
        return 1;
    }

    // Aqui vocÃª pode ler ou escrever no arquivo

    fclose(arquivo); // Fecha o arquivo
    return 0;
}